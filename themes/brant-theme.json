  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
      ],
      "settings": {
        "foreground": "#867f78"
      }
    },
    {
      "name": "Comment with extra info",
      "scope": [
        "comment support.other.namespace.php",
        "comment punctuation.separator.inheritance.php",
        "comment support.class.php",
        "comment keyword.other"
      ],
      "settings": {
        "foreground": "#8d6767"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "Colors",
      "scope": [
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
